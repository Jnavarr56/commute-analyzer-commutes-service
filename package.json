{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --maxWorkers=1",
    "test:watch": "jest --watchAll --maxWorkers=1",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/server.js",
    "build": "rimraf dist && babel src --out-dir dist",
    "start": "NODE_ENV=production npm run build && node dist/server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "npm run lint:fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2"
  }
}
